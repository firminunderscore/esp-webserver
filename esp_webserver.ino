#include <WiFi.h>
#include <time.h>

const char *ssid = "SSID";
const char *password = "PASSWORD";

WiFiServer server(80);

const char *ntpServer = "pool.ntp.org";
const long gmtOffset_sec = 3600;
const int daylightOffset_sec = 3600;

IPAddress local_IP(192, 168, 1, 250);
IPAddress gateway(192, 168, 1, 254);
IPAddress subnet(255, 255, 255, 0);
IPAddress primaryDNS(192, 168, 1, 200);

void setup()
{
  Serial.begin(115200);

  if (!WiFi.config(local_IP, gateway, subnet, primaryDNS))
  {
    Serial.println("Error: IP Config");
  }

  Serial.print("Wi-Fi connecting... ");
  Serial.println(ssid);
  WiFi.begin(ssid, password);
  while (WiFi.status() != WL_CONNECTED)
  {
    delay(500);
    Serial.print(".");
  }

  Serial.println("\nWiFi connected.");
  Serial.println("IP: ");
  Serial.println(WiFi.localIP());

  configTime(gmtOffset_sec, daylightOffset_sec, ntpServer);
  Serial.println("NTP sync...");

  server.begin();
}

String getCurrentTime()
{
  struct tm timeinfo;
  if (!getLocalTime(&timeinfo))
  {
    return "Error : can't get current time !";
  }
  char timeStr[64];
  strftime(timeStr, sizeof(timeStr), "%Y-%m-%d %H:%M:%S", &timeinfo);
  return String(timeStr);
}

void loop()
{
  WiFiClient client = server.available();

  if (client)
  {
    String currentLine = "";

    while (client.connected())
    {
      if (client.available())
      {
        char c = client.read();
        Serial.write(c);

        if (c == '\n' && currentLine.length() == 0)
        {
          String currentTime = getCurrentTime();

          client.println("HTTP/1.1 200 OK");
          client.println("Content-type:text/html");
          client.println("Connection: close");
          client.println();
          client.println("<!DOCTYPE html><html>");
          client.println("<head><title>Your're connected to a random ESP32</title></head>");
          client.println("<body><h1>Yeah, this webserver is a fucking ESP32</h1>");
          client.println("<img src=\"data:image/jpeg;base64,\"");
          client.println("<br><p>Current time (Europe/Paris) : " + currentTime + "</p>");
          client.println("<p>Read more about it <a href=\"https://github.com/firminunderscore/esp-webserver/\">here</a>\.</p>");
          client.println("</body></html>");
          client.println();
          break;
        }

        if (c == '\n')
        {
          currentLine = "";
        }
        else if (c != '\r')
        {
          currentLine += c;
        }
      }
    }

    client.stop();
  }
}